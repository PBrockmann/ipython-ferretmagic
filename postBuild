#!/bin/bash

#==================================================================
wget https://github.com/NOAA-PMEL/PyFerret/releases/download/v7.3/pyferret-7.3-RHEL7-64.tar.gz
tar xvfz pyferret-7.3-RHEL7-64.tar.gz

wget ftp://ftp.pmel.noaa.gov/ferret/pub/data/fer_dsets.tar.gz
tar xvfz fer_dsets.tar.gz

#==================================================================
. ferret_paths.sh
